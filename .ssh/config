# Work git account
Host github.com
    HostName github.com
    User git
    IdentityFile ~/.ssh/id_rsa

# Personal git account
# Matches a git repo which is cloned with 
# `git clone git@github.com-chriswuj:chriswuj/xxxx.git`
Host github.com-chriswuj
    HostName github.com
    User git
    IdentityFile ~/.ssh/chriswuj_rsa

